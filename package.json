{
  "name": "ant-live-theme",
  "version": "0.1.0",
  "homepage": "http://mzohaibqc.github.io/antd-live-theme",
  "private": true,
  "dependencies": {
    "antd": "^3.4.0",
    "antd-theme-webpack-plugin": "1.0.1",
    "babel-plugin-import": "^1.7.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.3.1",
    "react-app-rewire-antd-theme": "1.0.4",
    "react-app-rewire-less": "^2.1.1",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.3.1",
    "react-scripts": "^1.1.4"
  },
  "watch": {
    "colors": {
      "patterns": [
        "src/styles",
        "generate-colors.js"
      ],
      "extensions": "less,js",
      "quiet": true,
      "legacyWatch": true
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build-colors": "cp public/color.less build/color.less",
    "build": "react-app-rewired build",
    "build-app": "run-p build-app build-colors",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "bundle-less": "^0.0.3",
    "gh-pages": "^1.1.0",
    "less-bundle-promise": "1.0.4",
    "npm-run-all": "^4.1.2",
    "npm-watch": "^0.3.0",
    "post-compile-webpack-plugin": "^0.1.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}